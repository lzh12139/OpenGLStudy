{
    "code-runner.executorMap": {
        "javascript": "node",
        "java": "cd $dir && javac $fileName && java $fileNameWithoutExt",
        "c": "cd $dir && gcc $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "cpp": "cd $dir && g++ $fileName -o $fileNameWithoutExt -Wall -g -static-libgcc -std=c++11 -Ic:/Users/HP/Desktop/dev-cpp/openGL/include -Lc:/Users/HP/Desktop/dev-cpp/openGL/lib -lglfw3dll -lopengl32 -lglad -lglfw3 && $dir$fileNameWithoutExt",
